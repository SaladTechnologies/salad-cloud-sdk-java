{
  "editor.formatOnSave": true,
  "editor.minimap.maxColumn": 120,
  "editor.renderWhitespace": "all",
  "editor.rulers": [120],
  "files.associations": {
    "CODEOWNERS": "ignore"
  },
  "git.branchProtection": ["main"],
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
